{
    "$schema": "http://mystik01.me/schema.json",
    "$id": "http://mystik01.me/schema.json",
    "type": "object",
    "title": "config",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "prefix": "?",
            "language": "en",
            "bot-name": "YOUR-BOT-NAME",
            "serverID": "SERVER-ID",
            "ModeratorRoles": [
                "RoleId1",
                "RoleId2"
            ],
            "EmbedAuthor": "Modmail",
            "Category": "Category-ID-For-Tickets",
            "LogChannel": "Channel-ID-to-log-tickets",
            "EmbedPfp": "https;//modmail.com/modmail.png",
            "EmbedColour": "#a60100",
            "token": "bot-token"
        }
    ],
    "required": [
        "prefix",
        "language",
        "bot-name",
        "serverID",
        "ModeratorRoles",
        "EmbedAuthor",
        "Category",
        "LogChannel",
        "EmbedPfp",
        "EmbedColour",
        "token"
    ],
    "properties": {
        "prefix": {
            "$id": "#/properties/prefix",
            "type": "string",
            "title": "The prefix schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "?"
            ]
        },
        "language": {
            "$id": "#/properties/language",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "en"
            ],
            "title": "The language schema",
            "enum": [
                "es",
                "en"
            ],
            "type": "string"
        },
        "bot-name": {
            "$id": "#/properties/bot-name",
            "type": "string",
            "title": "The bot-name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "YOUR-BOT-NAME"
            ]
        },
        "serverID": {
            "$id": "#/properties/serverID",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "SERVER-ID"
            ],
            "title": "The serverID schema",
            "maxLength": 18,
            "pattern": "[0-9]",
            "type": "string"
        },
        "ModeratorRoles": {
            "$id": "#/properties/ModeratorRoles",
            "default": [],
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                [
                    "RoleId1",
                    "RoleId2"
                ]
            ],
            "title": "The ModeratorRoles schema",
            "maxLength": 18,
            "pattern": "[0-9]",
            "type": "array",
            "additionalItems": true,
            "items": {
                "$id": "#/properties/ModeratorRoles/items",
                "anyOf": [
                    {
                        "$id": "#/properties/ModeratorRoles/items/anyOf/0",
                        "type": "string",
                        "title": "The first anyOf schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": "",
                        "examples": [
                            "RoleId1",
                            "RoleId2"
                        ]
                    }
                ]
            }
        },
        "EmbedAuthor": {
            "$id": "#/properties/EmbedAuthor",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "Modmail"
            ],
            "title": "The EmbedAuthor schema",
            "maxLength": 18,
            "pattern": "[0-9]",
            "type": "string"
        },
        "Category": {
            "$id": "#/properties/Category",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "Category-ID-For-Tickets"
            ],
            "title": "The Category schema",
            "maxLength": 18,
            "pattern": "[0-9]",
            "type": "string"
        },
        "LogChannel": {
            "$id": "#/properties/LogChannel",
            "type": "string",
            "title": "The LogChannel schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Channel-ID-to-log-tickets"
            ]
        },
        "EmbedPfp": {
            "$id": "#/properties/EmbedPfp",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "https;//modmail.com/modmail.png"
            ],
            "title": "The EmbedPfp schema",
            "pattern": "https?:\/\/(www\.)?[-a-zA-Z0-9@:%._\+~#=]{1,256}\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_\+.~#?&//=]*)",
            "type": "string"
        },
        "EmbedColour": {
            "$id": "#/properties/EmbedColour",
            "type": "string",
            "title": "The EmbedColour schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "#a60100"
            ]
        },
        "token": {
            "$id": "#/properties/token",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "bot-token"
            ],
            "title": "The token schema",
            "maxLength": 59,
            "type": "string"
        }
    },
    "additionalProperties": true
}
